{
  "window.zoomLevel": 0,
  "editor.fontFamily": "Zed Sans",
  "terminal.integrated.fontFamily": "ZedMono Nerd Font",
  "editor.fontSize": 13,
  "terminal.integrated.fontSize": 13,
  "editor.fontLigatures": true,
  "terminal.external.osxExec": "/Applications/kitty.app",
  "terminal.external.linuxExec": "kitty",
  "terminal.integrated.scrollback": 20000,
  "terminal.integrated.cursorStyle": "underline",
  "editor.inlineSuggest.enabled": true,
  "extensions.ignoreRecommendations": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/static": true,
    "**/.next": true,
    "**/@remote-types": true,
    ".yarnrc.yml": true,
    "yarn.lock": true,
    "**/dependabot.yml": true,
    "**/commit-msg": true,
    "**/*.cjs": true,
    "**/*.svg": true,
    "**-lock.yaml": true,
    "**/cache": true,
    "**/.cache": true
  },
  "eslint.format.enable": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "eslint.run": "onSave",
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "npm.packageManager": "pnpm",
  "editor.minimap.enabled": false,
  "editor.glyphMargin": false,
  "editor.folding": false,
  "editor.foldingHighlight": false,
  "editor.lineNumbers": "off",
  "editor.tabSize": 2,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "solid",
  "git.decorations.enabled": false,
  "workbench.editor.decorations.badges": false,
  "workbench.editor.decorations.colors": false,
  "explorer.decorations.colors": false,
  "editor.guides.indentation": false,
  "breadcrumbs.enabled": false,
  "window.commandCenter": true,
  "explorer.confirmDelete": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "workbench.activityBar.location": "hidden",
  "workbench.sideBar.location": "right",
  "json.schemaDownload.enable": true,
  "workbench.startupEditor": "none",
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "workbench.colorTheme": "Tomorrow Night Blue",
  "workbench.colorCustomizations": {
    "editor.background": "#201A2F",
    "activityBar.background": "#201A2F",
    "statusBar.background": "#201A2F",
    "sideBar.background": "#201A2F",
    "tab.inactiveBackground": "#201A2F",
    "tab.border": "#201A2F",
    "editorOverviewRuler.border": "#201A2F",
    "editorGroupHeader.tabsBackground": "#201A2F",
    "menu.background": "#201A2F",
    "input.background": "#201A2F",
    "editorSuggestWidget.background": "#201A2F",
    "editorSuggestWidget.border": "#201A2F",
    "scrollbar.shadow": "#201A2F",
    "scrollbarSlider.background": "#201A2F",
    "scrollbarSlider.activeBackground": "#201A2F",
    "scrollbarSlider.hoverBackground": "#201A2F",
    "titleBar.activeBackground": "#201A2F",
    "editorSuggestWidget.selectedBackground": "#201A2F",
    "quickInput.background": "#201A2F",
    "editor.selectionBackground": "#181227",
    "editor.lineHighlightBackground": "#181227",
    "editorHoverWidget.background": "#181227",
    "editorSuggestWidget.selectedForeground": "#6a6367",
    "tab.inactiveForeground": "#6a6367"
  },
  "workbench.statusBar.visible": false
}
