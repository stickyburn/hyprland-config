{
  "window.zoomLevel": 0,
  "editor.fontFamily": "ZedMono Nerd Font",
  "terminal.integrated.fontFamily": "ZedMono Nerd Font",
  "editor.fontSize": 12.5,
  "terminal.integrated.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.inlineSuggest.enabled": true,
  "extensions.ignoreRecommendations": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/static": true,
    "**/.next": true,
    "**/@remote-types": true,
    ".yarnrc.yml": true,
    "yarn.lock": true,
    "*.md": true,
    "**/dependabot.yml": true,
    "**/commit-msg": true,
    "**/*.cjs": true,
    "**/*.svg": true,
    "**-lock.yaml": true,
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.codeActionsOnSave.mode": "all",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc"
  ],
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "npm.packageManager": "pnpm",
  "editor.minimap.enabled": false,
  "editor.glyphMargin": false,
  "editor.folding": false,
  "editor.foldingHighlight": false,
  "editor.lineNumbers": "off",
  "editor.tabSize": 2,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "solid",
  "terminal.integrated.cursorStyle": "underline",
  "git.decorations.enabled": false,
  "workbench.editor.decorations.badges": false,
  "workbench.editor.decorations.colors": false,
  "explorer.decorations.colors": false,
  "editor.guides.indentation": false,
  "breadcrumbs.enabled": false,
  "window.commandCenter": true,
  "explorer.confirmDelete": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "workbench.activityBar.location": "hidden",
  "workbench.sideBar.location": "right",
  "terminal.integrated.scrollback": 50000,
  "workbench.startupEditor": "none",
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "workbench.colorTheme": "Tomorrow Night Blue",
  "workbench.colorCustomizations": {
    "editor.background": "#1D1B28",
    "activityBar.background": "#1D1B28",
    "statusBar.background": "#1D1B28",
    "sideBar.background": "#1D1B28",
    "tab.inactiveBackground": "#1D1B28",
    "tab.border": "#1D1B28",
    "editorOverviewRuler.border": "#1D1B28",
    "editorGroupHeader.tabsBackground": "#1D1B28",
    "menu.background": "#1D1B28",
    "input.background": "#1D1B28",
    "editorSuggestWidget.background": "#1D1B28",
    "editorSuggestWidget.border": "#1D1B28",
    "scrollbar.shadow": "#1D1B28",
    "scrollbarSlider.background": "#1D1B28",
    "scrollbarSlider.activeBackground": "#1D1B28",
    "scrollbarSlider.hoverBackground": "#1D1B28",
    "titleBar.activeBackground": "#1D1B28",
    "editorSuggestWidget.selectedBackground": "#1D1B28",
    "quickInput.background": "#1D1B28",
    "editor.selectionBackground": "#110f1d",
    "editor.lineHighlightBackground": "#110f1d",
    "editorHoverWidget.background": "#110f1d",
    "editorSuggestWidget.selectedForeground": "#6a6367",
    "tab.inactiveForeground": "#6a6367"
  },
  "settingsSync.ignoredSettings": [
    "terminal.integrated.fontFamily",
    "terminal.integrated.fontSize",
    "editor.fontSize",
    "-window.zoomLevel",
    "window.zoomPerWindow"
  ],
  "terminal.integrated.tabs.enabled": false,
  "roo-cline.allowedCommands": [
    "git log",
    "git diff",
    "git show",
    "pnpm build"
  ],
  "roo-cline.deniedCommands": [],
  "workbench.statusBar.visible": false,
}