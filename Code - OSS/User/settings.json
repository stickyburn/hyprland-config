{
  "editor.fontFamily": "Liga Comic Shanns",
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.inlineSuggest.enabled": true,
  "extensions.ignoreRecommendations": true,
  "workbench.startupEditor": "none",
  "window.titleBarStyle": "custom",
  "files.exclude": {
    "**/dist": true,
    "**/node_modules": true,
  },
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.stickyScroll.enabled": true,
  "eslint.format.enable": true,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "editor.minimap.enabled": false,
  "editor.glyphMargin": false,
  "editor.folding": false,
  "editor.foldingHighlight": false,
  "editor.lineNumbers": "off",
  "editor.tabSize": 2,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "solid",
  "terminal.integrated.cursorStyle": "underline",
  "git.decorations.enabled": false,
  "workbench.editor.decorations.badges": false,
  "workbench.editor.decorations.colors": false,
  "explorer.decorations.colors": false,
  "editor.guides.indentation": false,
  "terminal.integrated.fontSize": 13,
  "window.commandCenter": false,
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "rust": false,
    "typescriptreact": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.activityBar.location": "top",
  "workbench.sideBar.location": "right",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": false,
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "Default Dark+",
  "terminal.integrated.scrollback": 5000,
  "workbench.colorCustomizations": {
    "editor.background": "#30293d",
    "activityBar.background": "#30293d",
    "statusBar.background": "#30293d",
    "sideBar.background": "#30293d",
    "tab.inactiveBackground": "#30293d",
    "tab.border": "#30293d",
    "editorOverviewRuler.border": "#30293d",
    "editorGroupHeader.tabsBackground": "#30293d",
    "menu.background": "#30293d",
    "input.background": "#30293d",
    "editorSuggestWidget.background": "#30293d",
    "editorSuggestWidget.border": "#30293d",
    "scrollbar.shadow": "#30293d",
    "scrollbarSlider.background": "#30293d",
    "scrollbarSlider.activeBackground": "#30293d",
    "scrollbarSlider.hoverBackground": "#30293d",
    "editorWidget.background": "#30293d",
    "editorWidget.border": "#30293d",
    "titleBar.activeBackground": "#30293d",
    "peekViewEditor.background": "#30293d",
    "peekViewResult.background": "#30293d",
    "editorInlayHint.background": "#30293d",
    "panel.background": "#30293d",
    "quickInput.background": "#30293d",
    "editorPane.background": "#30293d",
    "editorHoverWidget.background": "#30293d",
    "titleBar.inactiveBackground": "#30293d",
    "peekView.border": "#433c56",
    "editorSuggestWidget.selectedBackground": "#433c56",
    "tab.inactiveForeground": "#646476",
    "editor.selectionBackground": "#433c56",
    "editor.lineHighlightBackground": "#433c56"
  },
}